,YAAYf,KAAK,CAACgB,UAAN,CAAiBH,SAAjB,EAA4BC,EAAxC,GAA6C,GAAvD;AACD;;AACD,WAAOC,MAAP;AACD;;AACD,SAAOF,SAAP;AACD,C,CAED;;AACA,OAAO,SAASK,+BAAT,CAA0CC,OAA1C,EAAiG;AAAA,MAA9CC,aAA8C,uEAA9B,KAA8B;AAAA,MAAvBC,aAAuB,uEAAP,KAAO;AACtG,MAAIN,MAAM,GAAG,EAAb;;AACA,MAAII,OAAO,CAACG,OAAZ,EAAqB;AACnBP,IAAAA,MAAM,GAAG,oBAAmBI,OAAO,CAACI,IAA3B,GAAkC,YAAlC,GAAiDvB,KAAK,CAACwB,YAAN,CAAmBL,OAAO,CAACM,IAA3B,EAAiCC,KAA3F;;AACA,QAAIP,OAAO,CAACQ,OAAZ,EAAqB;AACnBZ,MAAAA,MAAM,IAAI,OAAOI,OAAO,CAACQ,OAAf,GAAyB,GAAnC;AACD;;AACDZ,IAAAA,MAAM,IAAI,gBAAgBI,OAAO,CAACS,SAAlC;AACD,GAND,MAMO;AACLb,IAAAA,MAAM,GAAGI,OAAO,CAACM,IAAjB;;AACA,QAAIN,OAAO,CAACQ,OAAZ,EAAqB;AACnBZ,MAAAA,MAAM,IAAI,OAAOI,OAAO,CAACQ,OAAf,GAAyB,GAAnC;AACD;;AACDZ,IAAAA,MAAM,IAAI,gBAAgBI,OAAO,CAACI,IAAlC;AACD;;AACD,MAAI,CAACH,aAAL,EAAoB;AAClB,QAAID,OAAO,CAACU,SAAZ,EAAuB;AACrBd,MAAAA,MAAM,IAAI,OAAOI,OAAO,CAACU,SAAzB;AACD,KAFD,MAEO;AACLd,MAAAA,MAAM,IAAI,eAAV;AACD;AACF;;AACD,MAAIM,aAAJ,EAAmB;AACjBN,IAAAA,MAAM,IAAI,qBAAqBH,gBAAgB,CAACO,OAAO,CAACN,SAAT,CAA/C;AACD;;AACD,SAAOE,MAAP;AACD","sourcesContent":["import {stats} from '../../data/stats'\n\n// Calcul du modificateur de caractéristique\nexport function getModifier (score) {\n  return Math.floor((score - 10) / 2)\n}\n\n// Ajoute un + devant les valeurs positives pour l'affichage\nexport function displayBonus (value) {\n  if (value >= 0) {\n    value = '+' + value\n  }\n  return value\n}\n\n// Affichage d'un score de caractéristiques sous la forme 16 (+3)\nexport function displayAbilityScore (score) {\n  let modifier = getModifier(score)\n  if (modifier >= 0) {\n    modifier = '+' + modifier\n  }\n  return score + ' (' + modifier + ')'\n}\n\n// Calcul du bonus de maîtrise en fonction du niveau\nexport function getProficiencyBonus (level) {\n  return Math.ceil(level / 4) + 1\n}\n\n// Affiche l'indice de dangerosité\nexport function displayChallenge (challenge, xp = false) {\n  let result = ''\n  if (stats.challenges[challenge]) {\n    result += stats.challenges[challenge].label\n    if (xp) {\n      result += ' (PX : ' + stats.challenges[challenge].xp + ')'\n    }\n    return result\n  }\n  return challenge\n}\n\n// Affiche le type, la taille et l'alignement\nexport function displayMonsterTypeSizeAlignment (monster, hideAlignment = false, showChallenge = false) {\n  let result = ''\n  if (monster.isSwarm) {\n    result = 'Nuée de taille '+ monster.size + ' composée ' + stats.monsterTypes[monster.type].swarm\n    if (monster.subtype) {\n      result += ' (' + monster.subtype + ')'\n    }\n    result += ' de taille ' + monster.swarmSize\n  } else {\n    result = monster.type\n    if (monster.subtype) {\n      result += ' (' + monster.subtype + ')'\n    }\n    result += ' de taille ' + monster.size\n  }\n  if (!hideAlignment) {\n    if (monster.alignment) {\n      result += ', ' + monster.alignment\n    } else {\n      result += ', non alignée'\n    }\n  }\n  if (showChallenge) {\n    result += ', Dangerosité : ' + displayChallenge(monster.challenge)\n  }\n  return result\n}\n"]}]}